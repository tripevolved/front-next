{
  "name": "app-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev & npx next-video sync -w",
    "dev:ssl": "next dev --port 3005 & npx local-ssl-proxy --key local.tripevolved.com.br-key.pem --cert local.tripevolved.com.br.pem --source 443 --target 3005 & npx next-video sync -w",
    "build": "next build",
    "start": "next start",
    "type:check": "tsc --project tsconfig.json --noEmit",
    "lint": "next lint",
    "lint:format": "prettier 'src/**/*.{ts,js,json,md,yml,yaml}' --write",
    "lint:script": "yarn type:check && yarn lint:format",
    "component:index": "node scripts/component-index.js",
    "component:new": "node scripts/component-new.js",
    "data:index": "node scripts/data-index.js",
    "test": "jest",
    "test:unit": "jest \".*spec\\.ts$\" --watch",
    "test:integration": "jest \".*test\\.tsx$\" --watch",
    "test:watch": "jest --watchAll",
    "slicemachine": "start-slicemachine",
    "ci:build": "yarn build",
    "ci:lint": "NODE_ENV=production yarn lint",
    "ci:test": "jest --silent --reporters=default --coverage --watchAll=false --env=jsdom --testResultsProcessor"
  },
